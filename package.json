{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run next:build",
    "commitlint": "commitlint",
    "commitlint:edit": "pnpm run commitlint --edit",
    "dev": "pnpm run next:dev & pnpm run prisma:dev & BROWSER=none pnpm run storybook:dev -p 9000",
    "eslint": "eslint",
    "eslint:all": "pnpm run eslint .",
    "husky": "husky",
    "husky:commit-msg": "pnpm run commitlint:edit",
    "husky:pre-commit": "pnpm run lint-staged",
    "lint": "pnpm run prettier:write:all && pnpm run eslint:all",
    "lint-staged": "lint-staged",
    "lint:check:all": "pnpm run prettier:check:all && pnpm run eslint:all",
    "next": "next",
    "next:build": "pnpm run next build",
    "next:dev": "pnpm run next dev",
    "next:start": "pnpm run next start",
    "prepare": "pnpm run husky && pnpm run prisma:generate",
    "prettier": "prettier",
    "prettier:check": "pnpm run prettier --check",
    "prettier:check:all": "pnpm run prettier:check .",
    "prettier:write": "pnpm run prettier --write",
    "prettier:write:all": "pnpm run prettier:write .",
    "prisma": "prisma",
    "prisma:dev": "pnpm run prisma studio",
    "prisma:generate": "pnpm run prisma generate",
    "prisma:lint": "pnpm run prisma format",
    "prisma:migrate": "pnpm run prisma migrate dev",
    "prisma:push": "pnpm run prisma db push",
    "start": "pnpm run next:start",
    "storybook": "storybook",
    "storybook:dev": "pnpm run storybook dev",
    "tsc": "tsc",
    "tsc:no-emit": "pnpm run tsc --noEmit",
    "typecheck": "pnpm run tsc:no-emit"
  },
  "dependencies": {
    "@durksteedjr/colors": "^2.0.3",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.15",
    "@ohanaui/react": "^1.0.2",
    "@prisma/client": "^5.20.0",
    "@radix-ui/react-visually-hidden": "^1.1.0",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/blocks": "^8.3.5",
    "@storybook/nextjs": "^8.3.5",
    "@storybook/react": "^8.3.5",
    "@tanstack/react-query": "^5.59.11",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "next": "^14.2.15",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "slugify": "^1.6.6",
    "tailwind-variants": "^0.2.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@eslint/js": "^9.12.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^5.20.0",
    "prop-types": "^15.8.1",
    "storybook": "^8.3.5",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  }
}
